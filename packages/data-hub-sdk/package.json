    "publish:npm": "yarn build && npm publish --access public"
  },
  "keywords": [
    "SDK"
  ],
  "dependencies": {
    "twitter-api-v2": "^1.15.0",
    "@subsocial/api": "latest"
  },
  "devDependencies": {
    "@swc/core": "^1.3.55",
    "tsup": "^6.7.0"
  }
}
