{
  "name": "@subsocial/data-hub-sdk",
  "version": "0.0.1",
  "description": "SDK for communication with Social DataHub service",
  "repository": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "scripts": {
    "build": "rm -rf build && tsup src/index.ts --format cjs,esm,iife --dts --minify",
